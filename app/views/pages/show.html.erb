<h2>Просмотр страницы</h2>
<div class="form-group">
  <%= link_to "Список страниц", pages_path, class: "btn btn-default" %>
</div>
<%= simple_form_for(@page, html: { class: 'form-horizontal' }) do |f| %>
  <%= f.input :title %>
  <%= f.input :keywords %>
  <%= f.input :description %>
  <%= f.input :url %>
  <%= f.input :content %>
<% end %>
