<%= simple_form_for(@page, html: { class: 'form-horizontal' }) do |f| %>
  <%= f.input :title %>
  <%= f.input :keywords %>
  <%= f.input :description %>
  <%= f.input :url %>
  <%= f.input :content %>
  <div class="form-group">
    <%= f.button :submit, value: "Сохранить", class: "btn btn-primary" %>
  </div>
<% end %>