.nav-breadcrumbs {
  height: 20px;
  background: #16568e; /* For browsers that do not support gradients */    
  background: -webkit-linear-gradient(left, #281851 , #00a5dd); /* For Safari 5.1 to 6.0 */
  background: -o-linear-gradient(right, #281851, #00a5dd); /* For Opera 11.1 to 12.0 */  
  background: -moz-linear-gradient(right, #281851, #00a5dd); /* For Firefox 3.6 to 15 */
  background: linear-gradient(to right, #281851 , #00a5dd); /* Standard syntax (must be last) */
  color: white;
  span {
    font-family: "CenturyGothic";
    margin-left: 20px;
    font-style: italic;
    font-weight: bold;
  }
}

